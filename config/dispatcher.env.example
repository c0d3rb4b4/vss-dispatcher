# VSS Dispatcher Configuration

# RabbitMQ Settings
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_NORMAL_QUEUE=vss.normal
RABBITMQ_PRIORITY_QUEUE=vss.priority
RABBITMQ_PREFETCH_COUNT=1

# VSS Service Settings
VSS_BASE_URL=http://localhost:8080
VSS_TIMEOUT=30
VSS_RETRY_COUNT=3
VSS_RETRY_DELAY=1.0

# Mount Paths
SAMBA_MOUNT_PATH=/mnt/samba
CONFIG_MOUNT_PATH=/mnt/config

# Application Settings
LOG_LEVEL=INFO
CHECK_INTERVAL=0.1
